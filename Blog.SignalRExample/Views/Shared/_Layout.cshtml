<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    @Styles.Render("~/Content/css")
    
    @Scripts.Render("~/bundles/jquery")
    <script src="@Url.Content("~/Scripts/jquery.signalR-2.0.3.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/signalr/js")" type="text/javascript"></script>

    <script src="@Url.Content("~/Scripts/underscore.js")" type="text/javascript"></script>
        
    @RenderSection("HeadScripts", required:false)
</head>
<body>
    <div class="page">
        <header>
            <div id="title">
                <h1>Real Time Pricing</h1>
            </div>
        </header>
        <section id="main">
            @RenderBody()
        </section>
        <footer>
        </footer>
    </div>
</body>
</html>
